{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": ["warn"],
    "no-underscore-dangle": 0,
    "global-require": 0,
    "object-curly-spacing": ["error", "never"],
    "comma-dangle": ["warn", "never"],
    "quotes": ["error", "single", {"allowTemplateLiterals": true}],
    "import/no-unresolved": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "no-continue": 0,
    "new-cap": 0
  }
}
